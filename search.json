---
title: nil
---
{% assign items = site.pages %}
[
	{% for item in items %}{% if item.url != '/search.json' %}{% if item.url != '/css/screen.css' %}
		{
			"title"    : "{{ item.title }}",
			"content"  : {{ item.content | jsonify }},
			"url"      : {{ item.url | jsonify }}
		}{% unless forloop.last %},{% endunless %}{% endif %}{% endif %}
	{% endfor %}
]
