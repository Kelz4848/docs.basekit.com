---
searchable: false
---
{% assign items = site.pages %}
[
	{% for item in items %}{% if item.searchable == false %}
		{
			"title"     : "",
			"content"   : "",
			"url"       : "",
			"searchable": "{{ item.searchable }}"
		}{% else %}
		{
			"title"     : {{ item.title | jsonify }},
			"content"   : {{ item.content | jsonify }},
			"url"       : {{ item.url | jsonify }},
			"searchable": "{{ item.searchable }}"
		}{% endif %}{% unless forloop.last %},{% endunless %}
	{% endfor %}
]
